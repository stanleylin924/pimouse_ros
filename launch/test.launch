<launch>
  <node pkg="pimouse_ros" type="buzzer.py" name="buzzer" required="true" />
  <node pkg="pimouse_ros" type="lightsensors.py" name="lightsensors" required="true" />
  <node pkg="pimouse_ros" type="motors.py" name="motors" required="true" />
  <test pkg="pimouse_ros" type="travis_test_buzzer.py" test-name="test_buzzer" />
  <test pkg="pimouse_ros" type="travis_test_lightsensors.py" test-name="test_lightsensors" />
  <test pkg="pimouse_ros" type="travis_test_motors.py" test-name="test_motors" time-limit="60.0" />
</launch>
